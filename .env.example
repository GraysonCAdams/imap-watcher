# IMAP settings
IMAP_HOST=imap.example.com
IMAP_PORT=993
IMAP_TLS=true
IMAP_USER=user@example.com
IMAP_PASSWORD=supersecret
IMAP_FOLDERS=INBOX,Screened Out

# CardDAV settings
CARDDAV_BASE_URL=https://carddav.example.com
CARDDAV_USERNAME=carddav-user
CARDDAV_PASSWORD=carddav-pass
CARDDAV_ADDRESSBOOK_PATH=/addressbooks/user/contacts/

# Contact group names
GROUP_SCREENED=Screened
GROUP_SCREENED_OUT=Screened Out

# Poll interval in seconds (if using polling fallback)
POLL_INTERVAL=30
